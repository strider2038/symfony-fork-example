{
    "type": "project",
    "license": "proprietary",
    "repositories": {
        "symfony": {
            "type": "git",
            "url": "https://github.com/nicolas-grekas/symfony.git"
        }
    },
    "require": {
        "php": "7.2.*",
        "ext-iconv": "*",
        "ext-json": "*",
        "acelaya/doctrine-enum-type": "2.*",
        "api-platform/api-pack": "1.*",
        "doctrine/collections": "1.*",
        "doctrine/doctrine-migrations-bundle": "1.*",
        "firebase/php-jwt": "5.*",
        "guzzlehttp/guzzle": "6.*",
        "martin-georgiev/postgresql-for-doctrine": "~0.9",
        "myclabs/php-enum": "1.*",
        "phpmetrics/phpmetrics": "^2.4",
        "ramsey/uuid": "3.*",
        "sensio/framework-extra-bundle": "5.*",
        "symfony-bundles/redis-bundle": "2.*",
        "symfony/console": "4.*",
        "symfony/flex": "1.*",
        "symfony/framework-bundle": "4.*",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "3.*",
        "symfony/proxy-manager-bridge": "4.*",
        "symfony/symfony": "dev-cache-fix",
        "symfony/yaml": "4.*"
    },
    "require-dev": {
        "behat/behat": "3.*",
        "behat/mink": "1.*",
        "behat/mink-browserkit-driver": "1.*",
        "behat/mink-extension": "2.*",
        "behat/symfony2-extension": "2.*",
        "behatch/contexts": "3.*",
        "doctrine/data-fixtures": "1.*",
        "enqueue/redis": "0.8.*",
        "hautelook/alice-bundle": "2.*",
        "phake/phake": "3.*",
        "phpunit/phpunit": "7.*",
        "rdx/behat-variables": "1.*",
        "roave/security-advisories": "dev-master",
        "symfony/debug": "4.*",
        "symfony/web-profiler-bundle": "4.*",
        "symplify/easy-coding-standard": "^4.5",
        "theofidry/alice-data-fixtures": "1.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
